{
  "name": "spearyx-monorepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "npm@9.2.0",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "preview": "turbo run preview",
    "deploy": "npm run build && npx wrangler deploy --config ./wrangler.toml"
  },
  "devDependencies": {
    "turbo": "latest",
    "pnpm": "latest"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
